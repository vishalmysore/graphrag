<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/frauddetection#"
     xml:base="http://www.semanticweb.org/frauddetection"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://www.semanticweb.org/frauddetection">
        <rdfs:comment>Comprehensive fraud detection ontology for GraphRAG with rich semantic relationships</rdfs:comment>
        <rdfs:label>Fraud Detection Knowledge Graph</rdfs:label>
    </owl:Ontology>

    <!-- =============================================== -->
    <!-- ANNOTATION PROPERTIES -->
    <!-- =============================================== -->
    
    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment"/>
    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label"/>

    <!-- =============================================== -->
    <!-- OBJECT PROPERTIES (Relationships) -->
    <!-- =============================================== -->

    <!-- Financial Relationships -->
    <owl:ObjectProperty rdf:about="#initiatedTransaction">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Transaction"/>
        <rdfs:label>initiated transaction</rdfs:label>
        <rdfs:comment>Links a person to a transaction they initiated</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#receivedTransaction">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Transaction"/>
        <rdfs:label>received transaction</rdfs:label>
        <rdfs:comment>Links a person to a transaction they received</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#ownsAccount">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Account"/>
        <rdfs:label>owns account</rdfs:label>
        <rdfs:comment>Links a person to an account they own</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#performedOn">
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="#Account"/>
        <rdfs:label>performed on</rdfs:label>
        <rdfs:comment>Links a transaction to the account it was performed on</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#usedDevice">
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="#Device"/>
        <rdfs:label>used device</rdfs:label>
        <rdfs:comment>Links a transaction to the device used</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#fromLocation">
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="#Location"/>
        <rdfs:label>from location</rdfs:label>
        <rdfs:comment>Geographic location where transaction originated</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#associatedWith">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#Person"/>
        <rdfs:label>associated with</rdfs:label>
        <rdfs:comment>Links people who have associations or relationships</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#flaggedBy">
        <rdfs:domain rdf:resource="#FraudCase"/>
        <rdfs:range rdf:resource="#FraudDetectionRule"/>
        <rdfs:label>flagged by</rdfs:label>
        <rdfs:comment>Links a fraud case to the detection rule that flagged it</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#involvedIn">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="#FraudCase"/>
        <rdfs:label>involved in</rdfs:label>
        <rdfs:comment>Links a person to a fraud case they are involved in</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#investigatedBy">
        <rdfs:domain rdf:resource="#FraudCase"/>
        <rdfs:range rdf:resource="#Investigator"/>
        <rdfs:label>investigated by</rdfs:label>
        <rdfs:comment>Links a fraud case to its investigator</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#relatedTo">
        <rdfs:domain rdf:resource="#FraudCase"/>
        <rdfs:range rdf:resource="#Transaction"/>
        <rdfs:label>related to</rdfs:label>
        <rdfs:comment>Links a fraud case to related transactions</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasRiskIndicator">
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="#RiskIndicator"/>
        <rdfs:label>has risk indicator</rdfs:label>
        <rdfs:comment>Links a transaction to its risk indicators</rdfs:comment>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#employedBy">
        <rdfs:domain rdf:resource="#Investigator"/>
        <rdfs:range rdf:resource="#Organization"/>
        <rdfs:label>employed by</rdfs:label>
        <rdfs:comment>Links an investigator to their organization</rdfs:comment>
    </owl:ObjectProperty>

    <!-- =============================================== -->
    <!-- DATA PROPERTIES (Attributes) -->
    <!-- =============================================== -->

    <!-- Person Properties -->
    <owl:DatatypeProperty rdf:about="#personID">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>person ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#fullName">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>full name</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#email">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>email</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#phoneNumber">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>phone number</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#riskScore">
        <rdfs:domain rdf:resource="#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>risk score</rdfs:label>
        <rdfs:comment>Calculated risk score for the person (0-100)</rdfs:comment>
    </owl:DatatypeProperty>

    <!-- Account Properties -->
    <owl:DatatypeProperty rdf:about="#accountNumber">
        <rdfs:domain rdf:resource="#Account"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>account number</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#accountBalance">
        <rdfs:domain rdf:resource="#Account"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>account balance</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#accountStatus">
        <rdfs:domain rdf:resource="#Account"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>account status</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#creationDate">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>creation date</rdfs:label>
    </owl:DatatypeProperty>

    <!-- Transaction Properties -->
    <owl:DatatypeProperty rdf:about="#transactionID">
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>transaction ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#amount">
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>amount</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#currency">
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>currency</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#timestamp">
        <rdfs:domain rdf:resource="#Transaction"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>timestamp</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#status">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>status</rdfs:label>
    </owl:DatatypeProperty>

    <!-- Device Properties -->
    <owl:DatatypeProperty rdf:about="#deviceID">
        <rdfs:domain rdf:resource="#Device"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>device ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#ipAddress">
        <rdfs:domain rdf:resource="#Device"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>IP address</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#deviceType">
        <rdfs:domain rdf:resource="#Device"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>device type</rdfs:label>
    </owl:DatatypeProperty>

    <!-- Location Properties -->
    <owl:DatatypeProperty rdf:about="#country">
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>country</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#city">
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>city</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#latitude">
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>latitude</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#longitude">
        <rdfs:domain rdf:resource="#Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>longitude</rdfs:label>
    </owl:DatatypeProperty>

    <!-- Fraud Case Properties -->
    <owl:DatatypeProperty rdf:about="#caseID">
        <rdfs:domain rdf:resource="#FraudCase"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>case ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#severity">
        <rdfs:domain rdf:resource="#FraudCase"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>severity</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#description">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>description</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#financialLoss">
        <rdfs:domain rdf:resource="#FraudCase"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>financial loss</rdfs:label>
    </owl:DatatypeProperty>

    <!-- =============================================== -->
    <!-- CLASSES (Class Hierarchy) -->
    <!-- =============================================== -->

    <!-- Top Level Classes -->
    <owl:Class rdf:about="#Agent">
        <rdfs:label>Agent</rdfs:label>
        <rdfs:comment>Any entity that can perform actions in the fraud detection system</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#FinancialEntity">
        <rdfs:label>Financial Entity</rdfs:label>
        <rdfs:comment>Any entity involved in financial operations</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Event">
        <rdfs:label>Event</rdfs:label>
        <rdfs:comment>Any event or occurrence in the system</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Resource">
        <rdfs:label>Resource</rdfs:label>
        <rdfs:comment>Any resource used in transactions</rdfs:comment>
    </owl:Class>

    <!-- Agent Subclasses -->
    <owl:Class rdf:about="#Person">
        <rdfs:subClassOf rdf:resource="#Agent"/>
        <rdfs:label>Person</rdfs:label>
        <rdfs:comment>An individual person in the system</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Customer">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label>Customer</rdfs:label>
        <rdfs:comment>A legitimate customer</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Fraudster">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label>Fraudster</rdfs:label>
        <rdfs:comment>A person identified as committing fraud</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Investigator">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label>Investigator</rdfs:label>
        <rdfs:comment>A fraud investigator</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Organization">
        <rdfs:subClassOf rdf:resource="#Agent"/>
        <rdfs:label>Organization</rdfs:label>
        <rdfs:comment>An organizational entity</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#FinancialInstitution">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label>Financial Institution</rdfs:label>
        <rdfs:comment>A bank or financial institution</rdfs:comment>
    </owl:Class>

    <!-- Financial Entity Subclasses -->
    <owl:Class rdf:about="#Account">
        <rdfs:subClassOf rdf:resource="#FinancialEntity"/>
        <rdfs:label>Account</rdfs:label>
        <rdfs:comment>A financial account</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#BankAccount">
        <rdfs:subClassOf rdf:resource="#Account"/>
        <rdfs:label>Bank Account</rdfs:label>
        <rdfs:comment>A traditional bank account</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#CreditCard">
        <rdfs:subClassOf rdf:resource="#Account"/>
        <rdfs:label>Credit Card</rdfs:label>
        <rdfs:comment>A credit card account</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#CryptocurrencyWallet">
        <rdfs:subClassOf rdf:resource="#Account"/>
        <rdfs:label>Cryptocurrency Wallet</rdfs:label>
        <rdfs:comment>A digital currency wallet</rdfs:comment>
    </owl:Class>

    <!-- Event Subclasses -->
    <owl:Class rdf:about="#Transaction">
        <rdfs:subClassOf rdf:resource="#Event"/>
        <rdfs:label>Transaction</rdfs:label>
        <rdfs:comment>A financial transaction</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Payment">
        <rdfs:subClassOf rdf:resource="#Transaction"/>
        <rdfs:label>Payment</rdfs:label>
        <rdfs:comment>A payment transaction</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Withdrawal">
        <rdfs:subClassOf rdf:resource="#Transaction"/>
        <rdfs:label>Withdrawal</rdfs:label>
        <rdfs:comment>A withdrawal transaction</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Transfer">
        <rdfs:subClassOf rdf:resource="#Transaction"/>
        <rdfs:label>Transfer</rdfs:label>
        <rdfs:comment>A transfer between accounts</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#OnlineTransaction">
        <rdfs:subClassOf rdf:resource="#Transaction"/>
        <rdfs:label>Online Transaction</rdfs:label>
        <rdfs:comment>A transaction performed online</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#FraudCase">
        <rdfs:subClassOf rdf:resource="#Event"/>
        <rdfs:label>Fraud Case</rdfs:label>
        <rdfs:comment>A fraud investigation case</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#IdentityTheftCase">
        <rdfs:subClassOf rdf:resource="#FraudCase"/>
        <rdfs:label>Identity Theft Case</rdfs:label>
        <rdfs:comment>A case of identity theft</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#MoneyLaunderingCase">
        <rdfs:subClassOf rdf:resource="#FraudCase"/>
        <rdfs:label>Money Laundering Case</rdfs:label>
        <rdfs:comment>A case of money laundering</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#CreditCardFraudCase">
        <rdfs:subClassOf rdf:resource="#FraudCase"/>
        <rdfs:label>Credit Card Fraud Case</rdfs:label>
        <rdfs:comment>A case of credit card fraud</rdfs:comment>
    </owl:Class>

    <!-- Resource Subclasses -->
    <owl:Class rdf:about="#Device">
        <rdfs:subClassOf rdf:resource="#Resource"/>
        <rdfs:label>Device</rdfs:label>
        <rdfs:comment>A device used for transactions</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#Location">
        <rdfs:subClassOf rdf:resource="#Resource"/>
        <rdfs:label>Location</rdfs:label>
        <rdfs:comment>A geographic location</rdfs:comment>
    </owl:Class>

    <!-- Detection and Analysis Classes -->
    <owl:Class rdf:about="#RiskIndicator">
        <rdfs:label>Risk Indicator</rdfs:label>
        <rdfs:comment>An indicator of potential fraud risk</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#AnomalousVelocity">
        <rdfs:subClassOf rdf:resource="#RiskIndicator"/>
        <rdfs:label>Anomalous Velocity</rdfs:label>
        <rdfs:comment>Unusual transaction velocity pattern</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#UnusualLocation">
        <rdfs:subClassOf rdf:resource="#RiskIndicator"/>
        <rdfs:label>Unusual Location</rdfs:label>
        <rdfs:comment>Transaction from unusual geographic location</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#SuspiciousAmount">
        <rdfs:subClassOf rdf:resource="#RiskIndicator"/>
        <rdfs:label>Suspicious Amount</rdfs:label>
        <rdfs:comment>Transaction amount is suspicious</rdfs:comment>
    </owl:Class>

    <owl:Class rdf:about="#FraudDetectionRule">
        <rdfs:label>Fraud Detection Rule</rdfs:label>
        <rdfs:comment>A rule used to detect fraud</rdfs:comment>
    </owl:Class>

