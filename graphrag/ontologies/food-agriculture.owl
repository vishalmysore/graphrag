    <owl:Ontology rdf:about="http://www.semanticweb.org/food">
        <rdfs:comment>Food Supply Chain and Agriculture Ontology for GraphRAG</rdfs:comment>
        <rdfs:label>Food & Agriculture Ontology</rdfs:label>
