    <!-- OBJECT PROPERTIES -->
    <owl:ObjectProperty rdf:about="#grows">
        <rdfs:domain rdf:resource="#Farm"/>
        <rdfs:range rdf:resource="#Crop"/>
        <rdfs:label>grows</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#suppliedBy">
        <rdfs:range rdf:resource="#Distributor"/>
        <rdfs:label>supplied by</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#containsIngredient">
        <rdfs:domain rdf:resource="#Recipe"/>
        <rdfs:range rdf:resource="#Ingredient"/>
        <rdfs:label>contains ingredient</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasAllergen">
        <rdfs:range rdf:resource="#Allergen"/>
        <rdfs:label>has allergen</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#certifiedBy">
        <rdfs:range rdf:resource="#Certification"/>
        <rdfs:label>certified by</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#serves">
        <rdfs:domain rdf:resource="#Restaurant"/>
        <rdfs:range rdf:resource="#Dish"/>
        <rdfs:label>serves</rdfs:label>
