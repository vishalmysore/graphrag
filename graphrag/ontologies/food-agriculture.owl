    <owl:ObjectProperty rdf:about="#ratedBy">
        <rdfs:range rdf:resource="#Customer"/>
        <rdfs:label>rated by</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#locatedIn">
        <rdfs:range rdf:resource="#Location"/>
        <rdfs:label>located in</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sourcedFrom">
        <rdfs:domain rdf:resource="#Ingredient"/>
        <rdfs:range rdf:resource="#Farm"/>
        <rdfs:label>sourced from</rdfs:label>
