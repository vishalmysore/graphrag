<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/supplychain#"
     xml:base="http://www.semanticweb.org/supplychain"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://www.semanticweb.org/supplychain">
        <rdfs:comment>Supply Chain Network Ontology for GraphRAG</rdfs:comment>
        <rdfs:label>Supply Chain Ontology</rdfs:label>
    </owl:Ontology>

    <!-- OBJECT PROPERTIES -->
    <owl:ObjectProperty rdf:about="#suppliesTo">
        <rdfs:domain rdf:resource="#Supplier"/>
        <rdfs:range rdf:resource="#Organization"/>
        <rdfs:label>supplies to</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#manufactures">
        <rdfs:domain rdf:resource="#Manufacturer"/>
        <rdfs:range rdf:resource="#Product"/>
        <rdfs:label>manufactures</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#shipsTo">
        <rdfs:domain rdf:resource="#Distributor"/>
        <rdfs:range rdf:resource="#Location"/>
        <rdfs:label>ships to</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#dependsOn">
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="#Component"/>
        <rdfs:label>depends on</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#containsComponent">
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="#Component"/>
        <rdfs:label>contains component</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#storedIn">
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="#Warehouse"/>
        <rdfs:label>stored in</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#orderedBy">
        <rdfs:domain rdf:resource="#Order"/>
        <rdfs:range rdf:resource="#Organization"/>
        <rdfs:label>ordered by</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#deliveredBy">
        <rdfs:domain rdf:resource="#Shipment"/>
        <rdfs:range rdf:resource="#Carrier"/>
        <rdfs:label>delivered by</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#locatedIn">
        <rdfs:range rdf:resource="#Location"/>
        <rdfs:label>located in</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sourcedFrom">
        <rdfs:domain rdf:resource="#Component"/>
        <rdfs:range rdf:resource="#Supplier"/>
        <rdfs:label>sourced from</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#partOfOrder">
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="#Order"/>
        <rdfs:label>part of order</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasDisruption">
        <rdfs:domain rdf:resource="#SupplyChainNode"/>
        <rdfs:range rdf:resource="#Disruption"/>
        <rdfs:label>has disruption</rdfs:label>
    </owl:ObjectProperty>

    <!-- DATA PROPERTIES -->
    <owl:DatatypeProperty rdf:about="#supplierID">
        <rdfs:domain rdf:resource="#Supplier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>supplier ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#organizationName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>organization name</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#productID">
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>product ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#productName">
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>product name</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#quantity">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>quantity</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#unitPrice">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:label>unit price</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#leadTime">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>lead time (days)</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#riskLevel">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>risk level</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#country">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>country</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#orderDate">
        <rdfs:domain rdf:resource="#Order"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>order date</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#deliveryDate">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>delivery date</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#status">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>status</rdfs:label>
    </owl:DatatypeProperty>

    <!-- CLASSES -->
    <owl:Class rdf:about="#SupplyChainNode">
        <rdfs:label>Supply Chain Node</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Organization">
        <rdfs:subClassOf rdf:resource="#SupplyChainNode"/>
        <rdfs:label>Organization</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Supplier">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label>Supplier</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#RawMaterialSupplier">
        <rdfs:subClassOf rdf:resource="#Supplier"/>
        <rdfs:label>Raw Material Supplier</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#ComponentSupplier">
        <rdfs:subClassOf rdf:resource="#Supplier"/>
        <rdfs:label>Component Supplier</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Manufacturer">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label>Manufacturer</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Distributor">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label>Distributor</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Retailer">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label>Retailer</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Carrier">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label>Carrier</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Warehouse">
        <rdfs:subClassOf rdf:resource="#SupplyChainNode"/>
        <rdfs:label>Warehouse</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Product">
        <rdfs:label>Product</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#FinishedProduct">
        <rdfs:subClassOf rdf:resource="#Product"/>
        <rdfs:label>Finished Product</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Component">
        <rdfs:subClassOf rdf:resource="#Product"/>
        <rdfs:label>Component</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#ElectronicComponent">
        <rdfs:subClassOf rdf:resource="#Component"/>
        <rdfs:label>Electronic Component</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#MechanicalPart">
        <rdfs:subClassOf rdf:resource="#Component"/>
        <rdfs:label>Mechanical Part</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#RawMaterial">
        <rdfs:label>Raw Material</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Order">
        <rdfs:label>Order</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#PurchaseOrder">
        <rdfs:subClassOf rdf:resource="#Order"/>
        <rdfs:label>Purchase Order</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Shipment">
        <rdfs:label>Shipment</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Location">
        <rdfs:label>Location</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Disruption">
        <rdfs:label>Disruption</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#DelayDisruption">
        <rdfs:subClassOf rdf:resource="#Disruption"/>
        <rdfs:label>Delay Disruption</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#ShortageDisruption">
        <rdfs:subClassOf rdf:resource="#Disruption"/>
        <rdfs:label>Shortage Disruption</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#QualityIssue">
        <rdfs:subClassOf rdf:resource="#Disruption"/>
        <rdfs:label>Quality Issue</rdfs:label>
    </owl:Class>

