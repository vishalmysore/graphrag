<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/healthcare#"
     xml:base="http://www.semanticweb.org/healthcare"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    
    <owl:Ontology rdf:about="http://www.semanticweb.org/healthcare">
        <rdfs:comment>Clinical Healthcare System Ontology for GraphRAG</rdfs:comment>
        <rdfs:label>Healthcare Clinical Ontology</rdfs:label>
    </owl:Ontology>

    <!-- OBJECT PROPERTIES -->
    <owl:ObjectProperty rdf:about="#diagnosedWith">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Diagnosis"/>
        <rdfs:label>diagnosed with</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#treatedBy">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#MedicalProfessional"/>
        <rdfs:label>treated by</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#prescribedBy">
        <rdfs:domain rdf:resource="#Prescription"/>
        <rdfs:range rdf:resource="#Doctor"/>
        <rdfs:label>prescribed by</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#admittedTo">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Hospital"/>
        <rdfs:label>admitted to</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#performedProcedure">
        <rdfs:domain rdf:resource="#Doctor"/>
        <rdfs:range rdf:resource="#MedicalProcedure"/>
        <rdfs:label>performed procedure</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasSymptom">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="#Symptom"/>
        <rdfs:label>has symptom</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#indicatesDisease">
        <rdfs:domain rdf:resource="#Symptom"/>
        <rdfs:range rdf:resource="#Disease"/>
        <rdfs:label>indicates disease</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#treatsDisease">
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="#Disease"/>
        <rdfs:label>treats disease</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#worksAt">
        <rdfs:domain rdf:resource="#MedicalProfessional"/>
        <rdfs:range rdf:resource="#Hospital"/>
        <rdfs:label>works at</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#specializedIn">
        <rdfs:domain rdf:resource="#Doctor"/>
        <rdfs:range rdf:resource="#MedicalSpecialty"/>
        <rdfs:label>specialized in</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasSideEffect">
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="#SideEffect"/>
        <rdfs:label>has side effect</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#contraIndicatedWith">
        <rdfs:domain rdf:resource="#Medication"/>
        <rdfs:range rdf:resource="#Medication"/>
        <rdfs:label>contraindicated with</rdfs:label>
    </owl:ObjectProperty>

    <!-- DATA PROPERTIES -->
    <owl:DatatypeProperty rdf:about="#patientID">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>patient ID</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#fullName">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>full name</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#age">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:label>age</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#bloodType">
        <rdfs:domain rdf:resource="#Patient"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>blood type</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#dosage">
        <rdfs:domain rdf:resource="#Prescription"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>dosage</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#severity">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>severity</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#datePerformed">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:label>date performed</rdfs:label>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#licenseNumber">
        <rdfs:domain rdf:resource="#Doctor"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label>license number</rdfs:label>
    </owl:DatatypeProperty>

