    <owl:Ontology rdf:about="http://www.semanticweb.org/enterprise">
        <rdfs:comment>Corporate Knowledge Management Ontology for GraphRAG</rdfs:comment>
        <rdfs:label>Enterprise Knowledge Management Ontology</rdfs:label>
    </owl:Ontology>

    <!-- OBJECT PROPERTIES -->
    <owl:ObjectProperty rdf:about="#worksIn">
        <rdfs:domain rdf:resource="#Employee"/>
        <rdfs:range rdf:resource="#Department"/>
        <rdfs:label>works in</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#managedBy">
        <rdfs:domain rdf:resource="#Department"/>
        <rdfs:range rdf:resource="#Manager"/>
        <rdfs:label>managed by</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasSkill">
        <rdfs:domain rdf:resource="#Employee"/>
        <rdfs:range rdf:resource="#Skill"/>
        <rdfs:label>has skill</rdfs:label>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#contributedTo">
        <rdfs:domain rdf:resource="#Employee"/>
        <rdfs:range rdf:resource="#Project"/>
        <rdfs:label>contributed to</rdfs:label>
